  <%- include('components/navbar.ejs') %>
  <section class="hero">
    <div class="hero_bg">
      <div class="h_bg_two">
        <div class="h_bg_two_sub">
          <h1>An <span class="brand">Assured</span> Way To Deliver</h1>
          <div class="copy_slogan">
            <span class="brand">Swift.</span>
            <span class="brand">Safe.</span>
            <span class="brand">Secure.</span>
          </div>
        </div>
      </div>
