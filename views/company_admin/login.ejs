    <title>
     Company Login - Koogah Logistics
    </title>
  </head>
  <body id="roots" class="company_admin_pages">
    <div class="loader hd">
      <div class="loader_holder">
        <img src="../../../images/loader_icon.svg" width="100%" height="100%s"/>
      </div>
    </div>
    <div class="toast remove"></div>
    <div class="d-modal hide"></div>
    <section class="login_hero" id="admin_login_page">
      <div class="lh_hero_c">
        <div class="logo_place">
          <div class="overlay"></div>
          <div class="logo_p_lc">
            <img src='../../../images/logo_gold.svg' width="100%" height="100%"/>
          </div>
          <div class="logo_p_lc_two">
            <img src='../../../images/partner_logo.png' width="100%" height="100%"/>
          </div>
        </div>
        <div class="bg_place"></div>
      </div>
      <div class="lh_hero_c_top">
        <div class="lh_login_form_c">
          <div class="lg_login_sub_c">
            <div class="lg_login_header_c">
              <div class="lg_login_h_sub_c mb">
                <img src="../../../images/logo_header.svg" width="100%" height="100%" alt="koogah logo"/>
              </div>
              <div class="mb">
                <h2>Login to access your Dashboard</h2>
              </div>
            </div>
            <form
              class="wd-100"
              @submit.prevent="login"
              novalidate="true"
              >
             <div class="est_form_sub_hld">
              <div class="wd-100 est_form_drpdwn_cont">
                <input 
                  id="company_login_email"
                  name="company_login_email"
                  type="email"
                  class="form-control form_field"
                  placeholder="Email Address"
                  aria-required="true"
                  v-model="email"
                />
              </div>
             </div>
             <div class="est_form_content_holder">
              <div class="est_form_sub_hld">
                <div class="wd-100 est_form_drpdwn_cont">
                  <input 
                    id="company_setup_password"
                    name="company_setup_password"
                    :type="showPassword ? 'text' : 'password'"
                    class="form-control form_field"
                    placeholder="Password"
                    autocomplete="none"
                    v-model="password"
                  />
                </div>
                <div :class="showPassword ? 'password_icon eye_show' : 'password_icon eye_off' " v-on:click="togglePasswordReveal"></div>
              </div>
              <span class="error" v-if="formErrors.password">{{formErrors.password}}</span>
              <div>
                <p class="link brand font-small">Forgot password?</p>
              </div>
            </div>
            <br/>
            <div>
              <button class="btn_bg_dark" type="submit">Sign in</button>
            </div>
            </form>
          </div>
        </div>
      </div>
    </section>
    <script src="../../../js/loader.js"></script>
    <script src="https://unpkg.com/vue"></script>
    <script src="../../../js/toast.js"></script>
    <script src="../../../js/modal.js"></script>
    <script src="../../../js/company_admin/login.js"></script>
  </body>
</html>
